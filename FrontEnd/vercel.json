{
  "version": 2,
  "builds": [
    {
      "src": "Public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/Signup/(.*)",
      "dest": "/Public/Signup/$1"
    },
    {
      "src": "/Login/(.*)",
      "dest": "/Public/Login/$1"
    },
    {
      "src": "/Support/(.*)",
      "dest": "/Public/Support/$1"
    },
    {
      "src": "/Dashboard/(.*)",
      "dest": "/Public/Dashboard/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/Public/assets/$1"
    },
    {
      "src": "/Home/(.*)",
      "dest": "/Public/Home/$1"
    },
    {
      "src": "/Notifications/(.*)",
      "dest": "/Public/Notifications/$1"
    },
    {
      "src": "/FoodSuggestion/(.*)",
      "dest": "/Public/FoodSuggestion/$1"
    },
    {
      "src": "/Insights/(.*)",
      "dest": "/Public/Insights/$1"
    },
    {
      "src": "/Loading/(.*)",
      "dest": "/Public/Loading/$1"
    },
    {
      "src": "/HelpSupport/(.*)",
      "dest": "/Public/HelpSupport/$1"
    },
    {
      "src": "/ongoing/(.*)",
      "dest": "/Public/ongoing/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/Public/$1"
    }
  ]
} 