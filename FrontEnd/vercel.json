{
  "version": 2,
  "builds": [
    {
      "src": "Public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/Public/index.html"
    },
    {
      "src": "/Signup/(.*)",
      "dest": "/Public/Signup/$1"
    },
    {
      "src": "/Login/(.*)",
      "dest": "/Public/Login/$1"
    },
    {
      "src": "/Support/(.*)",
      "dest": "/Public/Support/$1"
    },
    {
      "src": "/Dashboard/(.*)",
      "dest": "/Public/Dashboard/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/Public/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/Public/$1"
    }
  ]
} 